{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vite",
    "dev:light": "cross-env NODE_OPTIONS='--max-old-space-size=2048' vite --mode development",
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "build:analyze": "cross-env NODE_OPTIONS='--max-old-space-size=8192' vite build --mode analyze",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite dist && npm run clean:cache",
    "clean:cache": "npx rimraf node_modules/.cache",
    "clean:all": "npm run clean && npm run clean:lock && npm install",
    "clean:lock": "rm -f package-lock.json",
    "dev:clean": "npm run clean && npm run dev",
    "dev:memory-check": "cross-env NODE_OPTIONS='--max-old-space-size=4096 --inspect' vite",
    "analyze": "npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.8.7",
    "@fontsource/material-symbols-outlined": "^5.2.23",
    "@google-cloud/local-auth": "^3.0.1",
    "@react-oauth/google": "^0.12.2",
    "axios": "^1.8.4",
    "cose": "^0.0.2",
    "cytoscape": "^3.32.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "epubjs": "^0.3.93",
    "framer-motion": "^12.15.0",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "lucide-react": "^0.545.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.2.1",
    "recoil": "^0.7.7",
    "toast": "^0.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.11.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
